#include <Servo.h>


void setup()
{
  Servo motor1;
  Servo motor2;
  motor1.attach(11);
  motor2.attach(12);
  motor1.write(30);
  motor2.write(30);
  delay(1000);
  motor1.write(60);
  motor2.write(60);
  delay(1000); 
  motor1.write(90);
  motor2.write(90);
  delay(1000); 
  motor1.write(120);
  motor2.write(120);
  delay(1000); 
  motor1.write(150);
  motor2.write(150);
  delay(1000); 
  motor1.detach();
  motor2.detach();
  
}

void loop()
{
  
}
